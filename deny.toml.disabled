# cargo-deny configuration - enforce security and license checks
# Minimal valid configuration for current cargo-deny: top-level keys are expected
advisories = { deny = ["RUSTSEC-0000-0000"], ignore = [] }
# Disallow yanked crates
yanked = { deny = true }

[licenses]
# Example: require that dependencies are compatible with MIT/Apache-2.0
# (leave default empty to not block for now)
